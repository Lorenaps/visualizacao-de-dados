{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {"url": "https://github.com/vega/vega/blob/master/docs/data/seattle-weather.csv"},
    "mark": "bar",
    "encoding": {
        "x": {
        "bin": true,
        "field": "precipitation", 
        "type": "quantitative"
        },
        "y": {
        "aggregate": "count",
        "field": "*",
        "type": "quantitative"
        }
    }
}
